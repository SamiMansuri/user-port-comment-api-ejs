<%- include('./partials/header.ejs', {title: "Update Profile", nav_links:true,
loggedUser:user}) %>
<h1>Edit Profile</h1>
<form
  id="editProfileForm"
  action="/users/<%= user._id %>?_method=PUT"
  method="POST"
>
  Name: <input type="text" class="name" name="name" /> <br />
  Username: <input type="text" class="username" name="username" /> <br />
  Email: <input type="text" class="email" name="email" /> <br />
  <button type="submit">Update</button>
</form>
<%- include('./partials/footer.ejs') %>
